{
    "name": "Cookie Manager",
    "version": "1.0",
    "description": "Understand how websites are interacting with your cookies",
    "permissions": ["cookies","activeTab","declarativeContent","storage", "http://*.com/"],
    "background": {
      "scripts": [ "background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_title": "Cookie Info"
    },
    "manifest_version": 2
  }