{
    "name": "Cookie_Manager",
    "version": "1.0",
    "description": "Understand how websites are interacting with your cookies",
    "permissions": ["activeTab","declarativeContent","storage"],
    "background": {
      "scripts": [ "background.js"],
      "persistent": false
    },
    "page_action": {
      "default_popup": "popup.html"
    },
    "manifest_version": 2
  }